Manager:
    MDScreen:
        name: "welcome"
        id: welcome
        MDBoxLayout:
            orientation: 'vertical'
            MDBoxLayout:
                orientation: 'vertical'
                size: root.width, root.height
                MDBoxLayout:
                    adaptive_height: True
                    MDLabel:
                        text: "AFIT TOURS"
                        #f"[color=#000000BB][/color]"
                        color: app.theme_cls.primary_color
                        halign: 'center'
                        valign: 'center'
                        #pos_hint: {"center_y": .5, "center_x": .5}
                        markup: True
                        font_style: 'H3'
                RelativeLayout:
                    size_hint_y: .4
                    MDTextButton:
                        text: "GET STARTED"
                        color: app.theme_cls.primary_color
                        pos_hint: {"center_y": .2, "center_x": .5}
                        on_release: app.root.current = 'login'
                           
    

    MDScreen:
        name: 'login'
        id: login
        MDBoxLayout:
            orientation: 'vertical'
            spacing:'5dp'
            size: root.width, root.height/4
            MDBoxLayout:
                size_hint: 1, .2
                #size: root.width, root.height/4
                Image:
                    source: 'afitlogo1.png'
                    allow_stretch: False
                    keep_ratio: False
                    size_hint: .4, .5
                MDLabel:
                    text: f"[color=#82cdeb]AFIT TOURS[/color]"
                    #color: app.theme_cls.primary_color
                    halign: 'center'
                    valign: 'center'
                    markup: True
                    font_style: 'H5'
                    size_hint:.6, .5

            MDBoxLayout:
                size_hint: 1, .7
                MDGridLayout:
                    cols: 2
                    Button:
                        text:'Departments'
                        size_hint:.4, .4
                        markup: True
                        font_style: "Button"
                        on_release: app.root.current = 'departments_1'
                    Button:
                        text:'Offices'
                        size_hint:.4, .4
                        markup: True
                        font_style: "Button"
                        on_release: root.show_popup('error')
                    Button:
                        text:'Places'
                        size_hint:.4, .4
                        markup: True
                        font_style: "Button"
                        on_release: app.root.current = 'Places'
                    Button:
                        text:'Our Guides'
                        size_hint:.4, .4
                        markup: True
                        font_style: "Button"
                        on_release: root.show_popup('error')
                    Button:
                        text:'Visiting'
                        size_hint:.4, .4
                        markup: True
                        font_style: "Button"
                        on_press: root.show_popup('visiting')
                    Button:
                        text:'Help'
                        size_hint:.4, .4
                        markup: True
                        font_style: "Button"
                        on_press: root.show_popup('help')
            MDBoxLayout:
                size_hint: 1, .1
                RelativeLayout:
                    size_hint_y: .4
                    MDTextButton:
                        text: "VISIT HOME"
                        color: app.theme_cls.primary_color
                        pos_hint: {"center_y": .5, "center_x": .5}
                        on_release: app.root.current = 'welcome'
    
    MDScreen:
        name: 'departments_1'
